{
    "dependencies": {
        "@thream/socketio-jwt": "^2.1.1",
        "bcryptjs": "^2.4.3",
        "celebrate": "^15.0.0",
        "class-transformer": "^0.4.0",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-async-errors": "^3.1.1",
        "jsonwebtoken": "^8.5.1",
        "mongodb": "^4.1.1",
        "mysql": "^2.18.1",
        "reflect-metadata": "^0.1.13",
        "socket.io": "^4.1.3",
        "tsyringe": "^4.6.0",
        "typeorm": "^0.2.37",
        "uuid": "^8.3.2"
    },
    "scripts": {
        "dev:server": "ts-node-dev --transpile-only ./src/shared/infra/http/server.ts",
        "dev:db": "docker start mysql_sync || docker run -d --name mysql_sync --net=host -e MYSQL_ROOT_PASSWORD=123456 -e MYSQL_DATABASE=syncvideo -e MYSQL_USER=admin -e MYSQL_PASSWORD=admin mysql:5.7",
        "dev": "yarn dev:db && yarn migration:run && yarn dev:server",
        "preprod": "./node_modules/.bin/typeorm migration:run",
        "prod": "tsc && node ./dist/server.js",
        "migration:run": "yarn typeorm migration:run",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f ./src/config/ormconfig.js"
    },
    "name": "sync-backend",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/uuid": "^8.3.1",
        "@types/ws": "^7.4.7",
        "ts-node-dev": "^1.1.8",
        "tsconfig-paths": "^3.11.0",
        "typescript": "^4.3.5"
    }
}
